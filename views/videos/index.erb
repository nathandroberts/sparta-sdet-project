<h1 id="videos" class="text-center"><%= @heading %></h1>
<% @videos.each do |vid| %>
<div class="row">
  <div class="col-md-12">
    <h3 class="text-center"><a href="/videos/<%= vid.id%>"><%= vid.title %> </a></h3>
  </div>
</div>
<div class="row">
  <div class="col-md-12">
    <a href="/videos/<%= vid.id%>"><img class="thumbnail vid_thumbnail" src="<%= vid.image %>" alt="<%= vid.body %>"></a>
  </div>
</div>
<% end %>
